const fmt = import("fmt")

struct User {
    name,
    age,
    job,
}

fn User.info(self) {
    fmt.Println("User.info")
    fmt.Println(self.name)
    fmt.Println(self.age)
    fmt.Println(self.job)
}

let u = User{name:"Sobri", age:99, job:"Programmer"}
u.info()


fn hello(what) {
    fmt.Println("Hello", what)
}

hello("kat programming language")